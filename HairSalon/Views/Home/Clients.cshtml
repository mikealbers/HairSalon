@{
    Layout = "_Layout";
}

<div class="jumbotron text-center">
    <h1>Clients</h1>
</div>
<div class="container text-center">
    <div class="row">
        <div class="col-md-6">
        <h3>Client Name</h3>
        </div>
        <div class="col-md-6">
        <h3>Stylist</h3>
        </div>
    </div>
    @foreach (var client in Model)
    {
        <div class="row list">
            <div class="col-md-6">
            <h4>@client.GetClientName()</h4>
            </div>
            <div class="col-md-6">
            <h4>@client.GetStylistName()</h4>
            </div>
        </div>
    }
    <h3><a href="/clients">Go to All CLIENTS</a></h3>
    <h3><a href="/stylists">Return to STYLISTS</a></h3>
</div>
